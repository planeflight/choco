

confection parse(age) {
    if (age >= 18) {
        gift "You are an adult";
    } elif (age >= 13) {
        gift "You are a teen";
    }
    gift "You are a kid";

}

confection hi(name, age) {
    print("Hi " + name + "! You are " + age + " years old!");
    gift parse(age);
}

